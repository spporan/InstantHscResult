<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.poran.instanthscresult.ResultViaMessage">
   <RelativeLayout
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:background="#14234f"
       android:layout_margin="50dp">
       <Button
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:id="@+id/psc"
           android:layout_marginLeft="30dp"
           android:layout_marginRight="30dp"
           android:layout_marginTop="30dp"
           android:text="PSC/EBT Result"
           android:onClick="getResult"
           android:textSize="25dp"
           android:background="@drawable/buttonstyle"
           android:textColor="#1a237e"/>
       <Button
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:id="@+id/jsc"
           android:layout_below="@+id/psc"
           android:layout_marginLeft="30dp"
           android:layout_marginRight="30dp"
           android:layout_marginTop="30dp"
           android:text="JSC/JDC Result"
           android:onClick="getResult"
           android:textSize="25dp"
           android:background="@drawable/buttonstyle"
           android:textColor="#1a237e"
           />
       <Button
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:id="@+id/ssc"
           android:layout_below="@+id/jsc"
           android:layout_marginLeft="30dp"
           android:layout_marginRight="30dp"
           android:layout_marginTop="30dp"
           android:text="SSC Result"
           android:onClick="getResult"
           android:textSize="25dp"
           android:background="@drawable/buttonstyle"
           android:textColor="#1a237e"
           />
       <Button
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:id="@+id/hsc"
           android:layout_below="@+id/ssc"
           android:layout_marginLeft="30dp"
           android:layout_marginRight="30dp"
           android:layout_marginTop="30dp"
           android:text="HSC Result"
           android:onClick="getResult"
           android:textSize="25dp"
           android:background="@drawable/buttonstyle"
           android:textColor="#1a237e"
           />
       <Button
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:id="@+id/nu"
           android:layout_below="@+id/hsc"
           android:layout_marginLeft="30dp"
           android:layout_marginRight="30dp"
           android:layout_marginTop="30dp"
           android:layout_marginBottom="30dp"
           android:text="NU Result"
           android:onClick="getResult"
           android:textSize="25dp"
           android:background="@drawable/buttonstyle"
           android:textColor="#1a237e"
           />
   </RelativeLayout>



</LinearLayout>
